{
  "name": "feature-flag-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "run:tests": "vitest",
    "run:coverage": "vitest run --coverage",
    "run:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc --noEmit",
    "run:pre-build": "yarn run:lint && cross-env CI=true yarn run:tests --silent",
    "build": "yarn run:pre-build && vite build",
    "preview": "vite preview"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "5.0.8",
    "@mui/icons-material": "5.14.3",
    "@mui/material": "5.14.5",
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.15.0",
    "swr": "2.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.0.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "20.5.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "4.0.3",
    "@vitest/coverage-istanbul": "0.34.3",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "jsdom": "22.1.0",
    "msw": "1.2.3",
    "prettier": "3.0.2",
    "typescript": "5.0.2",
    "vite": "4.4.5",
    "vitest": "0.34.1"
  }
}
